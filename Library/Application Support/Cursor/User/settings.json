{
    "workbench.startupEditor": "none",
    "terminal.integrated.fontFamily": "MesloLGS NF",
    "terminal.integrated.fontSize": 11,
    "editor.mouseWheelZoom": true,
    "editor.accessibilitySupport": "off",
    "editor.defaultFormatter": "GitHub.copilot",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.organizeImports": "explicit"
    },
    "[dart]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.rulers": [
            80
        ],
        "editor.selectionHighlight": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": "off"
    },
    "dart.flutterHotReloadOnSave": "all",
    "dart.openDevTools": "flutter",
    "git.autofetch": true,
    "git.confirmSync": false,
    "editor.scrollBeyondLastLine": false,
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.hlsearch": true,
    "vim.leader": "<space>",
    "vim.handleKeys": {
        "<space>": true
    },
    "vim.useCtrlKeys": true,
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "<leader>",
                "o",
                "f"
            ],
            "commands": [
                "workbench.action.files.openFile"
            ]
        },
        {
            "before": [
                "<leader>",
                "x",
                "x"
            ],
            "commands": [
                "workbench.action.closeActiveEditor"
            ]
        },
        {
            "before": [
                "<leader>",
                "x",
                "a"
            ],
            "commands": [
                "workbench.action.closeAllEditors"
            ]
        },
        {
            "before": [
                "<leader>",
                "x",
                "o"
            ],
            "commands": [
                "workbench.action.closeOtherEditors"
            ]
        },
        {
            "before": [
                "<leader>",
                "f",
                "f"
            ],
            "commands": [
                "workbench.action.findInFiles"
            ]
        },
        {
            "before": [
                "\\",
                "c",
                "c"
            ],
            "commands": [
                "workbench.panel.chat.view.copilot.focus"
            ]
        }
    ],
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "j"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "d"
            ],
            "after": [
                "d",
                "d"
            ]
        },
        {
            "before": [
                "<C-n>"
            ],
            "commands": [
                ":nohl"
            ]
        },
        {
            "before": [
                "f",
                "f"
            ],
            "commands": [
                "workbench.view.search.focus"
            ]
        },
        {
            "before": [
                "K"
            ],
            "commands": [
                "lineBreakInsert"
            ],
            "silent": true
        },
        {
            "before": [
                "<leader>",
                "w"
            ],
            "commands": [
                "workbench.action.splitEditor"
            ]
        },
        {
            "before": [
                "<leader>",
                "e"
            ],
            "commands": [
                "workbench.action.toggleSidebarVisibility"
            ]
        },
        {
            "before": [
                "<leader>",
                "c"
            ],
            "commands": [
                "editor.action.commentLine"
            ]
        },
        {
            "before": [
                "<leader>",
                "f"
            ],
            "commands": [
                "revealInExplorer"
            ]
        },
        {
            "before": [
                "<leader>",
                "r",
                "n"
            ],
            "commands": [
                "editor.action.rename"
            ]
        },
        {
            "before": [
                "<leader>",
                "r",
                "f"
            ],
            "commands": [
                "fileutils.renameFile"
            ]
        },
        {
            "before": [
                "<leader>",
                "o",
                "g"
            ],
            "commands": [
                "workbench.action.showAllSymbols"
            ]
        },
        {
            "before": [
                "<leader>",
                "o",
                "o"
            ],
            "commands": [
                "workbench.action.showEditorsInActiveGroup"
            ]
        },
        {
            "before": [
                "leader",
                "o",
                "p"
            ],
            "commands": [
                "workbench.action.quickOpen"
            ]
        },
        {
            "before": [
                "m",
                "a"
            ],
            "commands": [
                "numberedBookmarks.toggleBookmark1"
            ]
        },
        {
            "key": "' a",
            "command": "numberedBookmarks.jumpToBookmark1",
            "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
        }
    ],
    "vim.visualModeKeyBindings": [
        {
            "before": [
                "leader",
                "c"
            ],
            "commands": [
                "editor.action.commentLine",
                "extension.vim_escape"
            ]
        }
    ],
    "multiCommand.commands": [
        {
            "command": "multiCommand.toggleMaximizedPanelAndFocusTerminal",
            "sequence": [
                "workbench.action.toggleMaximizedPanel",
                "workbench.action.terminal.focus"
            ]
        }
    ],
    "window.commandCenter": false,
    "window.autoDetectColorScheme": true,
    "github.copilot.chat.localeOverride": "ko",
    "workbench.colorTheme": "Default Light Modern",
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "yaml.schemas": {
        "file:///Users/brad/.vscode/extensions/atlassian.atlascode-3.0.14/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
    },
    "cursor.cpp.disabledLanguages": [
        "plaintext",
        "markdown",
        "scminput"
    ],
    "github.copilot.enable": {
        "*": false,
        "plaintext": false,
        "markdown": false,
        "scminput": false
    }
}