{
  "title": "Change input source with Shift+Space",
  "rules": [
    {
      "description": "Change input source with Shift+Space",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "spacebar",
            "modifiers": {
              "mandatory": ["shift"]
            }
          },
          "to": [
            {
              "key_code": "spacebar",
              "modifiers": ["command"]
            }
          ]
        }
      ]
    }
  ]
}
